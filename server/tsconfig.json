{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "target": "es2022",
    "lib": ["es2023"],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "forceConsistentCasingInFileNames": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.server.tsbuildinfo",
    "outDir": "../dist/server",
    "rootDir": "./src",
    "baseUrl": "./"
  },
  "include": ["./src/**/*"],
  "exclude": ["node_modules"]
}
